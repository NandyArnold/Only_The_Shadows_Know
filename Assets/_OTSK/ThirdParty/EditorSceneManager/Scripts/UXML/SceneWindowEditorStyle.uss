.label {
    font-size: 20px;
    -unity-font-style: bold;
    -unity-text-outline-color: rgba(0, 0, 0, 0.83);
    -unity-text-outline-width: 0.6px;
    letter-spacing: 5px;
    margin-top: 3px;
    margin-right: 3px;
    margin-bottom: 3px;
    margin-left: 3px;
    padding-top: 3px;
    padding-right: 3px;
    padding-bottom: 3px;
    padding-left: 3px;
    color: rgb(132, 219, 234);
    -unity-font-definition: url("project://database/Library/unity%20default%20resources?fileID=10102&guid=0000000000000000e000000000000000&type=0#LegacyRuntime");
}

.title {
    font-size: 23px;
    -unity-text-align: middle-center;
    text-shadow: 5px 5px 0 rgba(28, 41, 74, 0.63);
    text-overflow: clip;
    -unity-font-style: bold;
    color: rgb(158, 187, 217);
    margin-top: 5px;
    margin-right: 5px;
    margin-bottom: 5px;
    margin-left: 5px;
    padding-top: 5px;
    padding-right: 5px;
    padding-bottom: 5px;
    padding-left: 5px;
    white-space: normal;
    background-color: rgba(41, 41, 120, 0.12);
    border-top-left-radius: 400px;
    border-top-right-radius: 400px;
    border-bottom-right-radius: 400px;
    border-bottom-left-radius: 400px;
    -unity-font-definition: url("project://database/Library/unity%20default%20resources?fileID=10102&guid=0000000000000000e000000000000000&type=0#LegacyRuntime");
}

.loadButton {
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
    align-self: center;
    margin-right: 3px;
    margin-top: 3px;
    margin-bottom: 3px;
    margin-left: 3px;
    padding-top: 3px;
    padding-right: 3px;
    padding-bottom: 3px;
    padding-left: 3px;
    font-size: 13px;
    align-content: center;
    align-items: stretch;
    width: 55px;
    height: 24px;
    background-color: rgb(45, 132, 83);
    transition-duration: 0.15s;
}

.loadButton:hover {
    scale: 1.15 1.15;
}

.removeButton {
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    border-bottom-left-radius: 4px;
    align-self: center;
    margin-right: 3px;
    margin-top: 3px;
    margin-bottom: 3px;
    margin-left: 3px;
    padding-top: 3px;
    padding-right: 3px;
    padding-bottom: 3px;
    padding-left: 3px;
    font-size: 13px;
    align-content: center;
    align-items: stretch;
    width: 24px;
    height: 24px;
    background-color: rgb(145, 23, 23);
    transition-duration: 0.15s;
}

.section {
    margin-top: 2px;
    margin-right: 2px;
    margin-bottom: 2px;
    margin-left: 2px;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
