}

.button:hover {
    scale: 1.08 1.08;
    -unity-font-style: bold;
}

.button {
    font-size: 13px;
    -unity-text-align: middle-center;
    margin-top: 3px;
    margin-right: 3px;
    margin-bottom: 3px;
    margin-left: 3px;
    padding-top: 6px;
    padding-right: 6px;
    padding-bottom: 6px;
    padding-left: 6px;
    width: 70%;
    white-space: normal;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    height: 80%;
    transition-duration: 0.2s;
    flex-wrap: wrap;
    opacity: 1;
    justify-content: center;
    align-self: center;
    -unity-font-style: bold;
}

.colorField {
    width: 40px;
    height: 30px;
    margin-top: 3px;
    margin-right: 3px;
    margin-bottom: 3px;
    margin-left: 3px;
    padding-top: 3px;
    padding-right: 3px;
    padding-bottom: 3px;
    padding-left: 3px;
    align-content: center;
    align-self: center;
}

.infoText {
