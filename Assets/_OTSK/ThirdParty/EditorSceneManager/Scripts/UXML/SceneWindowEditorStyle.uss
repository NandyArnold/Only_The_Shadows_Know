    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 5px;
    flex-wrap: wrap;
    background-color: var(--unity-colors-default-background);
    border-left-color: rgba(0, 0, 0, 0.44);
    border-right-color: rgba(0, 0, 0, 0.44);
    border-top-color: rgba(0, 0, 0, 0.44);
    border-bottom-color: rgba(0, 0, 0, 0.44);
    border-bottom-width: 2px;
    border-right-width: 1px;
    border-left-width: 1px;
}

.removeButton:hover {
    scale: 1.15 1.15;
}

.colorField {
    width: 30px;
    height: 30px;
    margin-top: 3px;
    margin-right: 3px;
